{
    "name": "oskar-koli/kirby-litespeed",
    "type": "kirby-plugin",
    "license": "MIT",
    "description": "Litespeed cache plugin for Kirby CMS",
    "autoload": {
        "psr-4": {
            "OskarKoli\\KirbyLitespeed\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Oskar Koli",
            "email": "oskar@koli.io"
        }
    ],
    "require": {
        "getkirby/composer-installer": "^1.1"
    },
    "require-dev": {
        "getkirby/cms": "^5.0"
    },
    "config": {
        "optimize-autoloader": true,
        "allow-plugins": {
            "getkirby/composer-installer": true
        }
    }
}
